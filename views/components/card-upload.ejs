<div class="card shadow col-lg-7 p-0">
  <label class="card-header h4">上傳</label>

  <div class="card-body row row-gap-3 p-4">
    <div class="col-md-8">
      <input
        class="form-control"
        id="file"
        type="file"
        accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
        title="upload"
        multiple
      />
    </div>

    <div class="col-md-4 d-grid">
      <button class="btn btn-danger" type="button" onclick="upload()">
        上傳
      </button>
    </div>
  </div>
</div>
